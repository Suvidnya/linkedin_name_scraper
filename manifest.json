{
    "manifest_version": 2,
    "name": "LinkedIn Scraper",
    "description": "scroll down the page",
    "version": "0.1",
    "permissions": [
        "activeTab"
        
    ],
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js": ["popup.js"]
        }
    ],
    "browser_action": {
        "default_icon": "CT logo.png",
        "default_popup": "popup.html"
    },
    "background": {
       "scripts": [
           
            "bgscript.js"
        ]
    }
    
}